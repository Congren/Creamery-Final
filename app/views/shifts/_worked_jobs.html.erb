<h3>Jobs Worked</h3>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Job</th>
      <th class="centered">Description</th>
    </tr>
  </thead>
  <tbody>
    <% @worked_jobs.each do |job| %>
      <tr>
        <td><%= link_to job.name, job_path(job) %></td>
        <td class="centered"><%= job.description %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= will_paginate @worked_jobs, :previous_label => "Previous&nbsp;", :next_label => "&nbsp;Next" %>



